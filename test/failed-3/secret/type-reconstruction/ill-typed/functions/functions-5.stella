// ERROR_UNEXPECTED_TYPE_FOR_EXPRESSION ERROR_NOT_A_FUNCTION
language core;

extend with #type-reconstruction;

fn twice(f : auto) -> auto {
  return fn(n : auto) {
    return (if Nat::iszero(n) then f(n) else f(f(n)))(n)
  }
}

fn main(n : auto) -> auto {
  return twice( fn(x : auto){ return n } )(n)
}
